<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/bootstrap-datepicker.min.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.Content("~/Content/sticky-footer.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Style.css")" rel="stylesheet" type="text/css"/>
    <script src="@Url.Content("~/Scripts/jquery-1.12.3.min.js")"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")"></script>
</head>
<body>
<header>
    <div class="container">
        <div class="row">
            <div id="topElement">
                <div class="col-lg-3">
                    <p>Logo Here</p>
                </div>
                <div class="col-lg-9">
                    <p>Banner</p>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                @Html.Partial("_Menu")
            </div>
        </div>
    </div>
</header>
<div class="jumbotron">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                @RenderBody()
            </div>
        </div>
    </div>
</div>
<footer>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <p>Copyright BITM @DateTime.Now All Rights Reserved Null-Point.</p>
            </div>
        </div>
    </div>
</footer>    
</body>
</html>